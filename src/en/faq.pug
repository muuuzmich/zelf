        -loc = rq('./src/en/loc.js');
        include ../components/faq_header
        .list-container
        .pageAlign.faqPage
            ul.faq__list
                -let qustions = rq('./src/en/faq.js');
                each q, category  in qustions
                    ul.faq__list__category
                        h1(id=category) #{qustions[category].title}
                        each question in q.questions
                            li.faq__list__category__item
                                .faq__list__category__item__title=question.text
                                .faq__list__category__item__content
                                    p !{question.answer}
        include ../components/modal
        script(src="https://cdnjs.cloudflare.com/ajax/libs/tocbot/4.1.1/tocbot.min.js")
